{
  "name": "lighting",
  "version": "0.0.0",
  "description": "Sample app skeleton",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "nsp": "nsp check",
    "with-typescript": "ts-node --fast",
    "protractor": "npm run with-typescript -- ./node_modules/protractor/bin/protractor",
    "e20:install-latest-whl": "npm run with-typescript -- ./tools/configure-e20.ts",
    "e20:clear-database-and-restart": "npm run with-typescript -- ./tools/clean-and-restart-e20.ts",
    "update:webdriver": "webdriver-manager update",
    "postinstall": "bower install --save-exact",
    "lint": "eslint \"app/*/**.js\" \"test/*/**.js\"",
    "lint:ts": "tslint \"src/**/*.ts\"",
    "lint:ci": "npm run lint -- -o ./jslint.xml -f jslint-xml",
    "lint:fix": "npm run lint -- --fix",
    "test:types": "tsc -noEmit",
    "test:unit:ng1": "karma start",
    "test:unit:ng2:run": "node tests.js --dir src --files **/*.spec.ts",
    "test:unit:ng2": "nyc npm run test:unit:ng2:run",
    "test:unit:report": "istanbul report lcov cobertura",
    "test:unit": "npm-run-all test:unit:ng2 test:unit:ng1",
    "test:integration": "node tests.watch.js --dir src --files **/*.integration.ts",
    "pretest:e2e": "npm run update:webdriver",
    "test:e2e": "protractor",
    "test:e2e:ui": "npm run test:e2e -- --suite ui",
    "test:e2e:system": "npm run test:e2e -- --suite system",
    "test:e2e:integration1": "npm run test:e2e -- --suite integration1",
    "test:e2e:integration2": "npm run test:e2e -- --suite integration2",
    "test:e2e:integration3": "npm run test:e2e -- --suite integration3",
    "test:e2e:integration4": "npm run test:e2e -- --suite integration4",
    "test:e2e:integration5": "npm run test:e2e -- --suite integration5",
    "test:e2e:smoke": "npm run protractor -- ./e2e/smoke.config.ts",
    "test:e2e:acceptance": "npm run protractor -- ./e2e/acceptance.config.ts",
    "unit:ng1": "npm run test:unit:ng1 -- --auto-watch --no-single-run",
    "unit:ng2": "nodemon --watch src -e js,ts tests.watch.js --dir src --files **/*.spec.ts",
    "watch:test:integration": "nodemon --watch src -e js,ts tests.watch.js --dir src --files **/*.integration.ts",
    "clean": "rimraf lighting_ui/static && rimraf dist && rimraf wheelhouse",
    "bundle": "python setup.py bdist_wheel",
    "prebuild": "npm-run-all clean && pip install setuptools wheel",
    "build": "webpack",
    "build:dev": "npm run build -- --env.dev",
    "build:prod": "npm run build -- --env.prod",
    "postbuild": "npm run bundle",
    "prestart": "npm run clean",
    "start:lighting": "node ./start-lighting.js",
    "start:webpack-dev-server": "webpack-dashboard -- webpack-dev-server --config webpack.dashboard.js --env.dev --env.proxy --content-base lighting_ui/static",
    "dev": "webpack-dev-server --config webpack.dashboard.js --env.dev --env.proxy --content-base lighting_ui/static",
    "start": "npm run start:webpack-dev-server",
    "start:windows": "npm run start:webpack-dev-server -- --env.windows",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "ci:merge:e2e": "npm-run-all test:types build:prod e20:install-latest-whl update:webdriver test:e2e:smoke",
    "ci:merge:unit": "npm-run-all test:types test:unit e20:clear-database-and-restart test:integration",
    "ci:build:master": "npm-run-all ci:merge:unit ci:merge:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abskr82/app-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abskr82/app-boilerplate/issues"
  },
  "homepage": "https://github.com/abskr82/app-boilerplate#readme",
  "dependencies": {
    "angular": "^1.6.3",
    "angular-animate": "^1.6.3",
    "angular-bootstrap": "^0.12.2",
    "angular-messages": "^1.6.3",
    "angular-sanitize": "^1.6.3",
    "angular-ui-router": "^0.4.2",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "moment-timezone": "^0.5.11",
    "ui-bootstrap": "^0.1.10"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "compression-webpack-plugin": "^0.3.2",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "ng-annotate-loader": "^0.2.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "script-loader": "^0.7.0",
    "style-loader": "^0.15.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-md5-hash": "0.0.5",
    "webpack-validator": "^2.3.0",
    "worker-loader": "^0.8.0"
  }
}
